{
	"$schema": "./dedale.schema.json",
	"autoUpdate": true,
	"notifyUpdate": true,
	"autoUpgrade": true,
	"notifyUpgrade": true,
	"defaultProvider": "deno.land/x",
	"defaultTemplate": "vscode-mod",
	"plugins": {
		"readme-generator": {
			"path": "https://github.com/dedale/plugins/std/readme-generator/mod.ts",
			"activated": true
		},
		"licence-generator": {
			"path": "https://github.com/dedale/plugins/std/licence-generator/mod.ts",
			"activated": true
		}
	},
	"providers": {
		"deno-x": "deno.land/x",
		"nest.land": "nest.land",
		"github": "github.com",
		"npm": "npm"
	}
}
